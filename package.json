{
  "name": "exhibit-plugin-uglify",
  "version": "0.1.0",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "chalk": "^1.1.3",
    "convert-source-map": "^1.2.0",
    "exhibit": "^0.8.4",
    "lodash": "^4.8.2",
    "uglify-js": "^2.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "cssnano": "^3.5.2",
    "del-cli": "^0.2.0",
    "eslint": "^3.16.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "test-exhibit-plugin": "^0.1.2"
  },
  "engines": {
    "node": ">=0.12"
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": "airbnb/base",
    "rules": {
      "no-console": 0
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "exhibitjs/exhibit-plugin-uglify",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "build:watch": "del dist && babel src --out-dir dist --watch --source-maps",
    "prepublish": "npm run build",
    "test": "test-exhibit-plugin"
  }
}
